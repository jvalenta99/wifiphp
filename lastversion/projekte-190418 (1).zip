    <script>

        $(document).ready(function(){
            toggleBackground(true, 1);
        });

        function toggleBackground(fadeOut, currentImage){
            var backgroundDiv = $("#backgroundDiv");

            if(fadeOut){

                switch(currentImage){
                    case 1: currentImage = 2; break;
                    case 2: currentImage = 1; break;
                }

                backgroundDiv.fadeOut(5000, function(){
                    backgroundDiv.css("background-image", "url('plugins/Customizing/assets/images/background_no_login_" + currentImage + ".jpg')");
                    toggleBackground(false, currentImage);
                });
            }

            else{
                backgroundDiv.fadeIn(5000, function(){
                    toggleBackground(true, currentImage);
                });
            }
        }

    </script>

