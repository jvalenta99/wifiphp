$table->increments('id');
$table->string('name');
$table->integer('anzahl');
$table->dateTime('created_at');

$table->unsignedInteger('user_FK'); pokud chci udelat FK
$table->timestamps();

kdyz muze byt prazdne:
->nullable($value = true);

kdyz musi neco obsahovat:
->nullable($value = false);

$table->engine = 'InnoDB';
$table->charset = 'utf8';
$table->collation = 'utf8_unicode_ci';


